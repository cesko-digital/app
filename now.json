{
    "name": "web",
    "version": 2,
    "alias": "cesko.digital",
    "builds": [
        { "src": "*.html", "use": "@now/static" },
        { "src": "prirucka/**", "use": "@now/static" },
        { "src": "assets/**", "use": "@now/static" },
        { "src": "package.json", "use": "@now/static-build" }
    ],
    "routes": [
        {
            "src": "/prirucka(.*)",
            "status": 302,
            "headers": { "Location": "https://github.com/cesko-digital/derisking-handbook/releases/tag/v1.0.6" }
        }
    ],
    "github": {
        "enabled": true,
        "silent": false
    },
    "regions": ["bru1","cdg1","lhr1"]
  }
