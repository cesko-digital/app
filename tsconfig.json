{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES5",
    "jsx": "react",
    "lib": [
      "dom",
      "es2015"
    ],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "allowJs": true
  },
  "exclude": [
    "node_modules",
    "public",
    ".cache"
  ]
}
